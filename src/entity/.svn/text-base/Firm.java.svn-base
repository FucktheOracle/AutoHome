package entity;

public class Firm {

	private Integer fID;
	private String fName;
	private String fContacts;
	private Integer fPhone;
	private Integer fCode;
	private String fAddress;

	public Integer getfID() {
		return fID;
	}

	public void setfID(Integer fID) {
		this.fID = fID;
	}

	public String getfName() {
		return fName;
	}

	public void setfName(String fName) {
		this.fName = fName;
	}

	public String getfContacts() {
		return fContacts;
	}

	public void setfContacts(String fContacts) {
		this.fContacts = fContacts;
	}

	public Integer getfPhone() {
		return fPhone;
	}

	public void setfPhone(Integer fPhone) {
		this.fPhone = fPhone;
	}

	public Integer getfCode() {
		return fCode;
	}

	public void setfCode(Integer fCode) {
		this.fCode = fCode;
	}

	public String getfAddress() {
		return fAddress;
	}

	public void setfAddress(String fAddress) {
		this.fAddress = fAddress;
	}
}
