package entity;

public class Client {

	private Integer clID;
	private String clName;
	private String clGen;
	private Integer clPhone;
	private String clIDNum;
	private String clAddress;

	public Integer getClID() {
		return clID;
	}

	public void setClID(Integer clID) {
		this.clID = clID;
	}

	public String getClName() {
		return clName;
	}

	public void setClName(String clName) {
		this.clName = clName;
	}

	public String getClGen() {
		return clGen;
	}

	public void setClGen(String clGen) {
		this.clGen = clGen;
	}

	public Integer getClPhone() {
		return clPhone;
	}

	public void setClPhone(Integer clPhone) {
		this.clPhone = clPhone;
	}

	public String getClIDNum() {
		return clIDNum;
	}

	public void setClIDNum(String clIDNum) {
		this.clIDNum = clIDNum;
	}

	public String getClAddress() {
		return clAddress;
	}

	public void setClAddress(String clAddress) {
		this.clAddress = clAddress;
	}
}
